<p id="notice"><%= notice %></p>

<h1>Property Property Types</h1>

<table>
  <thead>
    <tr>
      <th>Property type</th>
      <th>Property</th>
      <th>Sites</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @property_property_types.each do |property_property_type| %>
      <tr>
        <td><%= property_property_type.property_type %></td>
        <td><%= property_property_type.property %></td>
        <td><%= property_property_type.sites %></td>
        <td><%= link_to 'Show', property_property_type %></td>
        <td><%= link_to 'Edit', edit_property_property_type_path(property_property_type) %></td>
        <td><%= link_to 'Destroy', property_property_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

