<p id="notice"><%= notice %></p>

<h1>Land Shapes</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @land_shapes.each do |land_shape| %>
      <tr>
        <td><%= land_shape.name %></td>
        <td><%= land_shape.value %></td>
        <td><%= link_to 'Show', land_shape %></td>
        <td><%= link_to 'Edit', edit_land_shape_path(land_shape) %></td>
        <td><%= link_to 'Destroy', land_shape, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Land Shape', new_land_shape_path %>
