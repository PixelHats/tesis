<p id="notice"><%= notice %></p>

<h1>Shed Materials</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @shed_materials.each do |shed_material| %>
      <tr>
        <td><%= shed_material.name %></td>
        <td><%= shed_material.value %></td>
        <td><%= link_to 'Show', shed_material %></td>
        <td><%= link_to 'Edit', edit_shed_material_path(shed_material) %></td>
        <td><%= link_to 'Destroy', shed_material, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Shed Material', new_shed_material_path %>
