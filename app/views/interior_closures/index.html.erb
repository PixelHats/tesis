<p id="notice"><%= notice %></p>

<h1>Interior Closures</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @interior_closures.each do |interior_closure| %>
      <tr>
        <td><%= interior_closure.name %></td>
        <td><%= interior_closure.value %></td>
        <td><%= link_to 'Show', interior_closure %></td>
        <td><%= link_to 'Edit', edit_interior_closure_path(interior_closure) %></td>
        <td><%= link_to 'Destroy', interior_closure, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Interior Closure', new_interior_closure_path %>
