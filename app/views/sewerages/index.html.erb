<p id="notice"><%= notice %></p>

<h1>Sewerages</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sewerages.each do |sewerage| %>
      <tr>
        <td><%= sewerage.name %></td>
        <td><%= sewerage.value %></td>
        <td><%= link_to 'Show', sewerage %></td>
        <td><%= link_to 'Edit', edit_sewerage_path(sewerage) %></td>
        <td><%= link_to 'Destroy', sewerage, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sewerage', new_sewerage_path %>
