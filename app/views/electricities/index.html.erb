<p id="notice"><%= notice %></p>

<h1>Electricities</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @electricities.each do |electricity| %>
      <tr>
        <td><%= electricity.name %></td>
        <td><%= electricity.value %></td>
        <td><%= link_to 'Show', electricity %></td>
        <td><%= link_to 'Edit', edit_electricity_path(electricity) %></td>
        <td><%= link_to 'Destroy', electricity, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Electricity', new_electricity_path %>
