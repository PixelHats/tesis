<p id="notice"><%= notice %></p>

<h1>Enlargements Qualities</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @enlargements_qualities.each do |enlargements_quality| %>
      <tr>
        <td><%= enlargements_quality.name %></td>
        <td><%= enlargements_quality.value %></td>
        <td><%= link_to 'Show', enlargements_quality %></td>
        <td><%= link_to 'Edit', edit_enlargements_quality_path(enlargements_quality) %></td>
        <td><%= link_to 'Destroy', enlargements_quality, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Enlargements Quality', new_enlargements_quality_path %>
