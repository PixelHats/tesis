<p id="notice"><%= notice %></p>

<h1>Housing Qualities</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @housing_qualities.each do |housing_quality| %>
      <tr>
        <td><%= housing_quality.name %></td>
        <td><%= housing_quality.value %></td>
        <td><%= link_to 'Show', housing_quality %></td>
        <td><%= link_to 'Edit', edit_housing_quality_path(housing_quality) %></td>
        <td><%= link_to 'Destroy', housing_quality, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Housing Quality', new_housing_quality_path %>
